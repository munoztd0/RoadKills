library(rgdal)
myConvert_LV95_to_WGS84 = function(df) {
coordinates(df) = ~ long + lat
df@proj4string = CRS("+init=epsg:2056")
myCoords_WGS84 = spTransform(df, CRS("+init=epsg:4326"))
myCoords_WGS84@coords
}
